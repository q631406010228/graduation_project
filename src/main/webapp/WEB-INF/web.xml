<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>graduation_project</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	 <!-- 获得毕业设计进程的内容 -->
	<servlet>
		<servlet-name>getGP</servlet-name>
		<servlet-class>com.zr.action.GetGPAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getGP</servlet-name>
		<url-pattern>/getGP</url-pattern>
	</servlet-mapping>
	
	 <!-- 保存毕业设计进程的时间、内容 -->
	<servlet>
		<servlet-name>setGP</servlet-name>
		<servlet-class>com.zr.action.SetGPAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>setGP</servlet-name>
		<url-pattern>/setGP</url-pattern>
	</servlet-mapping>
	
	 <!-- 保存毕业设计进程的时间、内容 -->
	<servlet>
		<servlet-name>showGP</servlet-name>
		<servlet-class>com.zr.action.ShowGPAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showGP</servlet-name>
		<url-pattern>/showGP</url-pattern>
	</servlet-mapping>
		
</web-app>